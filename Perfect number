#include<stdio.h>
int main()
{
   int num,i=1,s=0;
   printf("Enter the number:");
   scanf("%d",&num);
   while(i<num)
   {
      if(num%i==0)
      {
         s=s+i;
      }
      i++;
   }
      if(s==num)
      
      {
         printf("perfect number");
      }
      else 
      printf("not a perfect number");
   return 0;
}
