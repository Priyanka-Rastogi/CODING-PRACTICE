#include<stdio.h>
int main()
{
   int num,i;
   printf("enter the number: ");
   scanf("%d",&num);
   int count=0;
   for(int i=2;i<num;i++)
   {
      if(num%2==0)
      count++;
   }
   if(count!=0)
   {
      printf("not a prime number \n");
   }
   else
   printf("prime number");
   return 0;
}
