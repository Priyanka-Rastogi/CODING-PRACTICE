#include<stdio.h>
int main()
{
   int a,b,c;
   printf("Enter the values of a,b,c");
   scanf("%d%d%d",&a,&b,&c);
   if(a+b>c &&b+c>a&&a+c>b)
   {
      printf("triangle is valid");
      
      if(a==b&&b==c&&c==a)
      {
      printf("triangle is equilateral");
      }
      else if(a==b||b==c||c==a)
      {
      printf("triangle is isoceles");
      }
      else
      printf("triangle is scalene");
   }
   else 
   printf("triangle is invalid");
   return 0;
}
