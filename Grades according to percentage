#include<stdio.h>
void main()
{
   float chem,phy,math,bio,comp,percentage;
   printf("enter the marks:  ");
   scanf("%f%f%f%f%f", &chem,&phy,&math,&bio,&comp);
   percentage=(chem+phy+math+bio+comp)/5;
   if(percentage>=90)
   {
      printf("grade A");
   }
   else if(percentage>=80)
   {
      printf("grade B");
   }
   else if(percentage>=70)
   {
      printf("grade C");
   }else if(percentage>=60)
   {
      printf("grade D");
   }else if(percentage>=40)
   {
      printf("grade E");
   }
   else
   {
      printf("grade F");
   }
}
